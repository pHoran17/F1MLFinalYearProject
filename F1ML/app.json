{
  "expo": {
    "name": "F1ML",
    "slug": "F1ML",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/f1mlicon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pHoran.f1ml",
      "buildNumber": "1.0.1"
    },
    "android": {
      "package": "com.pHoran.f1ml",
      "googleServicesFile":"./android/app/google-services.json",
      "versionCode":1,
      "permissions": ["READ_PHONE_STATE", "CAMERA", "READ_CONTACTS", "RECORD_AUDIO"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adapticon.png",
        "backgroundColor": "#ed1005"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
